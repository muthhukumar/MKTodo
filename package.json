{
  "name": "mktodo",
  "private": true,
  "version": "0.18.2",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "tauri": "tauri",
    "build:mac:prod": "tauri build --target aarch64-apple-darwin --config './src-tauri/tauri.conf.prod.json'",
    "build:mac": "yarn tauri build --target x86_64-apple-darwin  --config './src-tauri/tauri.conf.prod.json'",
    "build:android": "tauri android build --apk",
    "check-types": "tsc",
    "build:dmg": "tauri build --bundles dmg",
    "test": "vitest",
    "bump:minor": "npm version minor && node ./scripts/updateVersion.cjs && git add package.json ./src-tauri/tauri.conf.json ./src-tauri/tauri.conf.prod.json && git commit -m 'Bump version'",
    "bump:patch": "npm version patch && node ./scripts/updateVersion.cjs && git add package.json ./src-tauri/tauri.conf.json ./src-tauri/tauri.conf.prod.json && git commit -m 'Bump version'"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.0.5",
    "@tanstack/react-router": "^1.35.3",
    "@tanstack/router-devtools": "^1.35.3",
    "@tanstack/router-vite-plugin": "^1.35.4",
    "@tauri-apps/api": "^2.0.0-beta.12",
    "@tauri-apps/plugin-clipboard-manager": "^2.0.0-rc.0",
    "@tauri-apps/plugin-shell": ">=2.0.0-beta.0",
    "@tauri-apps/plugin-store": "^2.0.0-rc.0",
    "axios": "^1.7.2",
    "clsx": "^2.1.1",
    "moment": "^2.30.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.1",
    "react-icons": "^5.2.1",
    "uuid": "^10.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^2.0.0-beta.18",
    "@types/node": "^20.14.2",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.19",
    "internal-ip": "^7.0.0",
    "jsdom": "^25.0.0",
    "postcss": "^8.4.38",
    "tailwind-merge": "^2.3.0",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.0.2",
    "vite": "^5.0.0",
    "vite-tsconfig-paths": "^5.0.1",
    "vitest": "^2.0.5"
  }
}
